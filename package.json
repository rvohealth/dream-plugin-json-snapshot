{
  "type": "module",
  "name": "@rvoh/dream-plugin-json-snapshot",
  "version": "2.0.0",
  "description": "A plugin for extracting model attributes and associations into JSON",
  "author": "RVO Health",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rvohealth/dream-plugin-json-snapshot.git"
  },
  "license": "MIT",
  "main": "./dist/cjs/src/index.js",
  "module": "./dist/esm/src/index.js",
  "types": "./dist/types/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/src/index.d.ts",
      "import": "./dist/esm/src/index.js",
      "require": "./dist/cjs/src/index.js"
    }
  },
  "files": [
    "tsconfig.json",
    "src/**/*",
    "dist/**/*"
  ],
  "scripts": {
    "build": "echo \"building cjs...\" && rm -rf dist && npx tsc -p ./tsconfig.cjs.build.json && echo \"building esm...\" && npx tsc -p ./tsconfig.esm.build.json",
    "build:test-app": "rm -rf dist && echo \"building test app to esm...\" && npx tsc -p ./tsconfig.esm.build.test-app.json && echo \"building test app to cjs...\" && npx tsc -p ./tsconfig.cjs.build.test-app.json",
    "prepack": "pnpm build",
    "dream": "NODE_ENV=${NODE_ENV:-test} pnpm dreamts",
    "dreamts": "node --experimental-specifier-resolution=node --import ./bin/esm.js ./test-app/cli/index.ts",
    "spec": "vitest --config=./spec/vite.config.ts",
    "lint": "pnpm eslint --no-warn-ignored \"src/**/*.ts\" \"spec/**/*.ts\" \"test-app/**/*.ts\" && pnpm prettier . --check",
    "format": "pnpm prettier . --write"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@rvoh/dream": "^2.0.1",
    "@rvoh/dream-spec-helpers": "^2.0.0",
    "@types/node": "^22.5.1",
    "@types/pg": "^8",
    "@typescript-eslint/parser": "^8.48.1",
    "commander": "^12.1.0",
    "dotenv": "^16.4.5",
    "eslint": "^9.39.1",
    "kysely": "^0.28.7",
    "kysely-codegen": "~0.19.0",
    "pg": "^8.12.0",
    "prettier": "^3.3.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.48.1",
    "vitest": "^4.0.10"
  },
  "peerDependencies": {
    "@rvoh/dream": "^2.0.0"
  }
}
