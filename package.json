{
  "name": "@rvohealth/dream-plugin-json-snapshot",
  "version": "1.0.4",
  "description": "A plugin for extracting model attributes and associations into JSON",
  "main": "dist/src/index.js",
  "repository": "https://github.com/rvohealth/dream-plugin-json-snapshot.git",
  "author": "rvohealth",
  "license": "MIT",
  "files": [
    "tsconfig.json",
    "src/**/*",
    "dist/**/*"
  ],
  "scripts": {
    "publish": "yarn npm publish",
    "build": "echo \"building dream-spec-helpers...\" && rm -rf dist && npx tsc -p ./tsconfig.json",
    "prepack": "yarn build",
    "dream": "yarn dreamts",
    "dreamts": "npx ts-node --transpile-only -r ./test-app/conf/loadEnv.ts ./test-app/cli/index.ts",
    "spec": "APP_ROOT_PATH=$(pwd) NODE_ENV=test jest --runInBand --forceExit --config=./spec/jest.config.js",
    "lint": "yarn run eslint --no-warn-ignored \"src/**/*.ts\" \"spec/**/*.ts\" \"test-app/**/*.ts\" && yarn run prettier . --check",
    "format": "yarn run prettier . --write"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "devDependencies": {
    "@eslint/js": "=9.0.0",
    "@rvohealth/dream": "^0.10.18",
    "@rvohealth/dream-spec-helpers": "^0.0.8",
    "@types/jest": "^29.5.12",
    "@types/luxon": "^3.4.2",
    "@types/node": "^22.5.1",
    "@types/pg": "^8",
    "commander": "^12.1.0",
    "dotenv": "^16.4.5",
    "eslint": "=9.0.0",
    "jest": "^29.7.0",
    "jest-plugin-context": "^2.9.0",
    "kysely": "^0.27.4",
    "kysely-codegen": "^0.16.5",
    "luxon": "^3.5.0",
    "pg": "^8.12.0",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "typescript-eslint": "=7.18.0"
  },
  "packageManager": "yarn@4.4.1",
  "peerDependencies": {
    "@rvohealth/dream": "*"
  }
}
